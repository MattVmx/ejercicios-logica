Algoritmo Actividad_Extra_1
	
	Definir vector, vector2, i Como Entero
	
	Dimension vector[5]
	Dimension vector2[5]
	
	Para i=0 Hasta 4 Hacer
		vector[i] = Aleatorio(1,10)
		vector2[i] = Aleatorio(1,10)
	FinPara
	
	Para i=0 Hasta 4 Hacer
		Escribir Sin Saltar "[", vector[i], "]"
	FinPara
	
	Escribir " "
	
	Para i=0 Hasta 4 Hacer
		Escribir Sin Saltar "[", vector2[i], "]"
	FinPara
	
	Escribir " "
	
FinAlgoritmo
